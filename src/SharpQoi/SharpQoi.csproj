<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;net5.0</TargetFrameworks>
		<LangVersion>10</LangVersion>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		
		<Authors>Eugene Antonov</Authors>
		<PackageVersion>1.0.0-beta.1</PackageVersion>
		<PackageProjectUrl>https://github.com/NUlliiON/QoiSharp</PackageProjectUrl>
		<PackageLicenseUrl>https://github.com/NUlliiON/QoiSharp/blob/master/LICENSE</PackageLicenseUrl>
		<PackageTags>QOI Fast Image Jpg Jpeg Bitmap Png Net6</PackageTags>
		<RepositoryUrl>https://github.com/NUlliiON/QoiSharp</RepositoryUrl>
		<Description>QoiSharp is an implementation of the QOI format for fast lossless image compression</Description>
		<PackageId>SharpQoi</PackageId>
		<Title>SharpQoi</Title>
	</PropertyGroup>

</Project>
